{
  "name": "@hidoo/gulp-plugin-image-placeholder",
  "version": "0.8.2",
  "description": "Plugin that add image of placeholder for gulp.",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "dependencies": {
    "fancy-log": "^1.3.3",
    "get-stream": "^5.0.0",
    "image-size": "^0.7.2",
    "ndarray": "^1.0.18",
    "plugin-error": "^1.0.1",
    "save-pixels": "^2.3.4",
    "through2": "^3.0.1",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "cross-env": "^5.2.0",
    "documentation": "^9.3.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "^4.0.2",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3"
  }
}
