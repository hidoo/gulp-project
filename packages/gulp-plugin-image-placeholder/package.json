{
  "name": "@hidoo/gulp-plugin-image-placeholder",
  "version": "0.19.0",
  "description": "Plugin that add image of placeholder for gulp.",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "dependencies": {
    "fancy-log": "1.3.3",
    "get-stream": "6.0.1",
    "image-size": "1.0.0",
    "ndarray": "1.0.19",
    "plugin-error": "1.0.1",
    "save-pixels": "2.3.4",
    "through2": "4.0.2",
    "vinyl": "2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/register": "7.13.16",
    "babel-preset-power-assert": "3.0.0",
    "cross-env": "7.0.3",
    "documentation": "13.2.5",
    "file-type": "16.5.1",
    "get-pixels": "3.3.2",
    "mocha": "8.4.0",
    "npm-run-all": "4.1.5",
    "pixelmatch": "5.2.1",
    "power-assert": "1.6.1",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "gulp": ">=4.0.2"
  }
}
