{
  "name": "@hidoo/gulp-plugin-image-placeholder",
  "version": "0.15.0",
  "description": "Plugin that add image of placeholder for gulp.",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "dependencies": {
    "fancy-log": "1.3.3",
    "get-stream": "5.1.0",
    "image-size": "0.8.3",
    "ndarray": "1.0.19",
    "plugin-error": "1.0.1",
    "save-pixels": "2.3.4",
    "through2": "3.0.1",
    "vinyl": "2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/register": "7.9.0",
    "babel-preset-power-assert": "3.0.0",
    "cross-env": "7.0.2",
    "documentation": "12.1.4",
    "file-type": "14.1.4",
    "get-pixels": "3.3.2",
    "mocha": "7.1.1",
    "npm-run-all": "4.1.5",
    "pixelmatch": "5.1.0",
    "power-assert": "1.6.1",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "gulp": ">=4.0.2"
  }
}
