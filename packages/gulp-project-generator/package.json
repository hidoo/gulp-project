{
  "name": "@hidoo/gulp-project-generator",
  "version": "0.12.0",
  "description": "Command line tool that generate gulp project scaffold.",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "bin": {
    "gulp-project-generator": "bin/index.js"
  },
  "files": [
    "bin/",
    "lib/",
    "template/"
  ],
  "scripts": {
    "start": "yarn generate ../../examples/dev --verbose --force",
    "generate": "cross-env NODE_ENV=development babel-node ./src/index.js",
    "prebuild": "rimraf ./lib",
    "build": "babel ./src --out-dir ./lib",
    "test": "cross-env NODE_ENV=test mocha ./test/**/*.test.js"
  },
  "keywords": [
    "gulp",
    "scaffold"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-preset-power-assert": "^3.0.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@hidoo/handlebars-helpers": "^0.6.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "eslint": "^6.1.0",
    "glob": "^7.1.4",
    "glob-parent": "^5.0.0",
    "handlebars": "^4.1.2",
    "inquirer": "^6.2.2",
    "mkdirp": "^0.5.1"
  }
}
