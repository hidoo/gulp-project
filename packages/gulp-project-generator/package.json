{
  "name": "@hidoo/gulp-project-generator",
  "version": "0.19.0",
  "description": "Command line tool that generate gulp project scaffold.",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "gulp-project-generator": "bin/index.js"
  },
  "files": [
    "bin/",
    "src/",
    "template/"
  ],
  "scripts": {
    "start": "yarn generate ../../examples/dev --verbose --force",
    "generate": "cross-env NODE_ENV=development node ./src/index.js",
    "test": "cross-env NODE_ENV=test mocha ./test/**/*.test.js"
  },
  "keywords": [
    "gulp",
    "scaffold"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "7.0.3",
    "mocha": "8.4.0"
  },
  "dependencies": {
    "@hidoo/handlebars-helpers": "0.8.2",
    "babel-eslint": "10.1.0",
    "chalk": "4.1.1",
    "commander": "7.2.0",
    "eslint": "7.28.0",
    "fs-extra": "10.0.0",
    "glob": "7.1.7",
    "glob-parent": "6.0.0",
    "handlebars": "4.7.7",
    "inquirer": "8.1.0",
    "mkdirp": "1.0.4"
  }
}
