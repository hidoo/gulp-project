{
  "name": "@hidoo/gulp-project-sample",
  "version": "0.0.0",
  "description": "Sample project for gulp.",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development gulp",
    "dev:build": "cross-env NODE_ENV=development npm-run-all -s static:clean static:build",
    "prod": "cross-env NODE_ENV=production gulp",
    "prod:build": "cross-env NODE_ENV=production npm-run-all -s static:clean static:build",
    "static:clean": "gulp clean",
    "static:build": "gulp build",
    "static:watch": "gulp watch",
    "test": "npm-run-all -s test:*",
    "test:unit": "cross-env NODE_ENV=test mocha ./src/js/**/*.test.js --opts ./src/js/mocha.opts"
  },
  "keywords": [
    "gulp",
    "sample"
  ],
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hidoo/gulp-task-build-css-stylus": "^0.0.0",
    "@hidoo/gulp-task-build-html-handlebars": "^0.0.0",
    "@hidoo/gulp-task-build-js-browserify": "^0.0.0",
    "@hidoo/gulp-task-build-sprite-image": "^0.0.0",
    "@hidoo/gulp-task-build-sprite-svg": "^0.0.0",
    "@hidoo/gulp-task-optimize-image": "^0.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "cross-env": "^5.2.0",
    "gulp": "^4.0.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.0",
    "rimraf": "^2.6.2"
  }
}
