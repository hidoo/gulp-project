{
  "name": "@hidoo/gulp-task-build-css-sass",
  "version": "0.10.0",
  "description": "Task that build css by sass for gulp.",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "task"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-preset-power-assert": "^3.0.0",
    "cross-env": "^5.2.0",
    "documentation": "^11.0.0",
    "gulp": "^4.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@hidoo/gulp-util-error-handler": "^0.10.0",
    "autoprefixer": "^9.6.0",
    "css-mqpacker": "^7.0.0",
    "fancy-log": "^1.3.3",
    "gulp-gzip": "^1.4.2",
    "gulp-header": "^2.0.7",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "node-sass-magic-importer": "^5.3.2",
    "postcss-csso": "^3.0.0",
    "postcss-uncss": "^0.17.0",
    "postcss-url": "^8.0.0",
    "uncss": "^0.17.0"
  },
  "peerDependencies": {
    "gulp": "^4.0.2"
  }
}
