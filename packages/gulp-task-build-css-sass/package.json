{
  "name": "@hidoo/gulp-task-build-css-sass",
  "version": "0.15.0",
  "description": "Task that build css by sass for gulp.",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "task"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/register": "7.9.0",
    "babel-preset-power-assert": "3.0.0",
    "cross-env": "7.0.2",
    "documentation": "12.3.0",
    "gulp": "4.0.2",
    "mocha": "7.1.1",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@hidoo/gulp-util-error-handler": "^0.15.0",
    "autoprefixer": "9.7.6",
    "css-mqpacker": "7.0.0",
    "fancy-log": "1.3.3",
    "gulp-gzip": "1.4.2",
    "gulp-header": "2.0.9",
    "gulp-if": "3.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "node-sass-magic-importer": "5.3.2",
    "postcss-csso": "4.0.0",
    "postcss-uncss": "0.17.0",
    "postcss-url": "8.0.0",
    "uncss": "0.17.3"
  },
  "peerDependencies": {
    "gulp": ">=4.0.2"
  }
}
