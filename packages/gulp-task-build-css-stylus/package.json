    "@babel/register": "^7.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "cross-env": "^5.2.0",
    "documentation": "^8.1.2",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@hidoo/gulp-util-error-handler": "^0.4.0",
    "autoprefixer": "^9.1.3",
    "css-mqpacker": "^7.0.0",
    "fancy-log": "^1.3.2",
    "gulp-gzip": "^1.4.2",
