{
  "name": "@hidoo/gulp-task-build-html-handlebars",
  "version": "0.18.0",
  "description": "Task that build html by handlebars for gulp.",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "npm-run-all -p build:*",
    "build:lib": "babel ./src --out-dir ./lib",
    "build:doc": "documentation readme ./src/index.js -q -s API --markdown-toc false --document-exported",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "gulp",
    "task"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "@babel/register": "7.13.14",
    "babel-preset-power-assert": "3.0.0",
    "cross-env": "7.0.3",
    "documentation": "13.2.0",
    "gulp": "4.0.2",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@hidoo/data-from": "0.3.1",
    "@hidoo/gulp-util-error-handler": "^0.18.0",
    "@hidoo/handlebars-helpers": "0.8.1",
    "gulp-data": "1.3.1",
